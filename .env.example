PORT=3000
NODE_ENV=development
LOG_LEVEL=info

BOOTSTRAP_STRICT=false

MYSQL_ENABLED=true
MYSQL_URL=mysql://root:root@localhost:3306/farmlokal
MYSQL_POOL_LIMIT=10

REDIS_ENABLED=true
REDIS_URL=redis://localhost:6379

CACHE_ENABLED=true
PRODUCTS_LIST_CACHE_TTL_SECONDS=30
PRODUCTS_STATS_CACHE_TTL_SECONDS=60
CACHE_LOCK_TTL_MS=2000
CACHE_ADMIN_ENABLED=false

RATE_LIMIT_ENABLED=false
RATE_LIMIT_WINDOW_SECONDS=60
RATE_LIMIT_MAX=120

METRICS_ENABLED=false
METRICS_PATH=/metrics

AUTH0_ENABLED=false
AUTH0_SECRET=replace_this_with_long_random_string
AUTH0_BASE_URL=http://localhost:3000
AUTH0_CLIENT_ID=replace_this
AUTH0_CLIENT_SECRET=replace_this
AUTH0_ISSUER_BASE_URL=https://replace_this.us.auth0.com

AUTH0_M2M_ENABLED=false
AUTH0_M2M_TOKEN_URL=https://replace_this.us.auth0.com/oauth/token
AUTH0_M2M_CLIENT_ID=replace_this
AUTH0_M2M_CLIENT_SECRET=replace_this
AUTH0_M2M_AUDIENCE=replace_this
AUTH0_M2M_SCOPE=

EXTERNAL_A_ENABLED=false
EXTERNAL_A_URL=https://example.com

WEBHOOK_B_ENABLED=false
WEBHOOK_B_SECRET=replace_this_with_shared_secret
